<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <display-name>meeto-v2</display-name>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>

    <servlet-mapping>
        <servlet-name>RegServlet</servlet-name>
        <url-pattern>/RegServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoneyServlet</servlet-name>
        <url-pattern>/MoneyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TopicsServlet</servlet-name>
        <url-pattern>/TopicsServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>IdeasServlet</servlet-name>
        <url-pattern>/IdeasServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PortfolioServlet</servlet-name>
        <url-pattern>/PortfolioServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>TransactionsServlet</servlet-name>
        <url-pattern>/TransactionsServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SharesServlet</servlet-name>
        <url-pattern>/SharesServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HallofFameServlet</servlet-name>
        <url-pattern>/HallofFameServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RootServlet</servlet-name>
        <url-pattern>/RootServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>FacebookServlet</servlet-name>
        <url-pattern>/FacebookServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AwebSocketServlet</servlet-name>
        <url-pattern>/AwebSocketServlet</url-pattern>
    </servlet-mapping>

    <servlet>

        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegServlet</servlet-name>
        <servlet-class>RegServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>

    </servlet>

    <servlet>

        <servlet-name>MoneyServlet</servlet-name>
        <servlet-class>MoneyServlet</servlet-class>

    </servlet>

    <servlet>

        <servlet-name>TopicsServlet</servlet-name>
        <servlet-class>TopicsServlet</servlet-class>

    </servlet>

    <servlet>

        <servlet-name>IdeasServlet</servlet-name>
        <servlet-class>IdeasServlet</servlet-class>

    </servlet>

    <servlet>

        <servlet-name>PortfolioServlet</servlet-name>
        <servlet-class>PortfolioServlet</servlet-class>

    </servlet>

    <servlet>

        <servlet-name>TransactionsServlet</servlet-name>
        <servlet-class>TransactionsServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>SharesServlet</servlet-name>
        <servlet-class>SharesServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>HallofFameServlet</servlet-name>
        <servlet-class>HallofFameServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>RootServlet</servlet-name>
        <servlet-class>RootServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>FacebookServlet</servlet-name>
        <servlet-class>FacebookServlet</servlet-class>
    </servlet>

    <servlet>

        <servlet-name>AwebSocketServlet</servlet-name>
        <servlet-class>AwebSocketServlet</servlet-class>
    </servlet>

</web-app>
